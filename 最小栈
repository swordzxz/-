class MinStack:

    def __init__(self):
        """
        initialize your data structure here.
        """
        self.stack = []
    def push(self, x):
        """
        :type x: int
        :rtype: void
        """
        if x is None:
            pass
        else :
            self.stack.append(x)

    def pop(self):
        """
        :rtype: void
        """
        if self.stack is None:
            return error
        else:
            self.stack.pop(-1)

    def top(self):
        """
        :rtype: int
        """
        if self.stack is None:
            return error
        else:
            self.stack[-1]

    def getMin(self):
        """
        :rtype: int
        """
        if self.stack is None:
            return error
        else:
            return min(self.stack)


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
